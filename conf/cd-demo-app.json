{
  "id": "/cd-demo-app",
  "cmd": "jekyll serve",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "agrzeskowiak/new-cicd-demo:latest"
    },
    "portMappings": [
      {
        "containerPort": 4000,
        "hostPort": 0,
        "protocol": "tcp",
        "name": "jenkins-deployed-app"
      }
    ]
  },
  "cpus": 1.0,
  "instances": 1,
  "labels": {
   "HAPROXY_0_VHOST": "agrzeskow-publicsl-mre9la02p9us-1025120264.us-west-2.elb.amazonaws.com",
   "HAPROXY_GROUP": "external",
   "lastChangedBy": "velocity-team@mesosphere.com"
  },
  "mem": 1024,
  "networks": [
    {
      "mode": "container/bridge",
      "labels": {}
    }
  ],
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
